pipeline {

    agent any

    stages {
		stage('set directory') {
			steps {
				sh '/var/jenkins_home/workspace/NetCore/Application'
            }
        }

		stage('restore') {
            steps {
				sh 'dotnet restore'
            }
        }

		stage('build'){
			steps {
				sh 'dotnet build'
			}
		}

		stage('test'){
			steps {
				sh 'dotnet test'
			}
		}
    }
}
